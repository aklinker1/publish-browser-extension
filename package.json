{
  "name": "publish-browser-extension",
  "description": "Easily publish chrome/browser extensions to their stores",
  "license": "MIT",
  "keywords": [
    "chrome-web-store",
    "firefox-addon-store",
    "chrome",
    "firefox",
    "addon",
    "store",
    "extension",
    "browser"
  ],
  "author": {
    "name": "Aaron Klinker",
    "url": "https://github.com/aklinker1"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "publish-extension": "dist/cli.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "compile": "tsc --noEmit",
    "build": "gulp build",
    "gulp": "gulp",
    "gulp:tasks": "gulp --tasks",
    "test": "echo 'TODO'",
    "test:watch": "echo 'TODO'",
    "dev": "env-cmd ./scripts/dev.sh",
    "dev:all": "env-cmd ./scripts/dev.sh all",
    "dev:chrome": "env-cmd ./scripts/dev.sh chrome",
    "dev:firefox": "env-cmd ./scripts/dev.sh firefox",
    "dev:help": "env-cmd ./scripts/dev.sh help",
    "generate-firefox-token": "env-cmd esno scripts/firefox-token.ts",
    "prepack": "gulp build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^16",
    "cli-highlight": "^2.1.11",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "node-fetch": "^2",
    "web-ext": "^6.8.0"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/node-fetch": "^2",
    "archiver": "^5.3.1",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.14.42",
    "esbuild-node-externals": "^1.4.1",
    "esno": "^0.16.3",
    "form-data": "^4.0.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.75.3",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "web-ext": "*"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/download",
        "download",
        "express",
        "safe-compare",
        "body-parser"
      ]
    }
  },
  "version": "1.0.0"
}